{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "symbol": {
      "type": "string"
    },
    "features": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "number"
        }
      }
    },
    "timestamps": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "date-time"
      }
    },
    "method": {
      "type": "string"
    },
    "options": {
      "type": "object"
    }
  },
  "required": [
    "symbol",
    "features"
  ]
}